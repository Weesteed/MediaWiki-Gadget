{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jquery": true,
        "es6": true
    },
    "globals": {
        "mw": true,
        "OO": true,
        "importScript": true,
        "wgULS": true,
        "wpTextbox1": true,
        "wpMinoredit": true,
        "wpSummary": true,
        "wpSave": true,
        "wpDiff": true
    },
    "rules": {
        "no-unused-labels": "off",
        "no-redeclare": "off",
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_"
            }
        ]
    }
}